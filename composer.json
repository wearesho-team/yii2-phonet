{
  "name": "wearesho-team/yii2-phonet",
  "description": "Yii2 Phonet integration",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kartvaik",
      "email": "roman.varkuta@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "kartavik/yii2-enum": "^2.0",
    "nesbot/carbon": "^1.36",
    "wearesho-team/phonet": "dev-hotfix/abstract-employee",
    "yiisoft/yii2": "~2.0.13",
    "yiisoft/yii2-phpunit": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Wearesho\\Phonet\\Yii\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wearesho\\Phonet\\Yii\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.1"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:wearesho-team/phonet.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:Horat1us/yii2-phpunit.git"
    }
  ],
  "scripts": {
    "lint": "phpcs --standard=PSR2 ./src ./tests",
    "phpcbf": "phpcbf --standard=PSR2 ./src ./tests",
    "cover": "phpunit --coverage-clover=coverage.xml",
    "test": "phpunit"
  }
}
